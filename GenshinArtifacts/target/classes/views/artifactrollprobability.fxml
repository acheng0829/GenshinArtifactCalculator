            <ComboBox fx:id="SubstatType3" layoutX="10.0" layoutY="148.0" prefHeight="25.0" prefWidth="141.0" />
            <ComboBox fx:id="SubstatType4" layoutX="10.0" layoutY="181.0" prefHeight="25.0" prefWidth="141.0" />
            <TextField fx:id="SubstatValue1" layoutX="151.0" layoutY="77.0" prefHeight="25.0" prefWidth="80.0" />
            <TextField fx:id="SubstatValue2" layoutX="151.0" layoutY="113.0" prefHeight="25.0" prefWidth="80.0" />
